<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
        <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
        <section name="entityFramework"
          type="System.TransportManager.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
          requirePermission="false"/>
    </configSections>

	<connectionStrings>
		<add name="DbConnect" connectionString="TransportManager.Data Source=.; Initial Catalog=my_ef_db; Integrated Security=True" providerName="System.TransportManager.Data.SqlClient"/>
		<add name="TelemetryDbConnect" connectionString="TransportManager.Data Source=.; Initial Catalog=telemetry; Integrated Security=True" providerName="System.TransportManager.Data.SqlClient" />
	</connectionStrings>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
	</startup>
	<entityFramework>
		<providers>
			<provider invariantName="System.TransportManager.Data.SqlClient" type="System.TransportManager.Data.Entity.SqlServer.SqlProviderTransportManager.Services, EntityFramework.SqlServer"/>
		</providers>
	</entityFramework>
</configuration>